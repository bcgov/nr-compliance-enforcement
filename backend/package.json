{
  "name": "nr-compliance-enforcement",
  "version": "2.0.127",
  "description": "BCGov devops quickstart. For reference, testing and new projects.",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:staged": "./node_modules/.bin/lint-staged",
    "make-badges": "istanbul-badges-readme --logo=jest --exitCode=1",
    "make-badges:ci": "npm run make-badges -- --ci",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bcgov/nr-compliance-enforcement.git"
  },
  "keywords": [
    "openshift",
    "pipeline",
    "node",
    "node",
    "js",
    "devops"
  ],
  "author": "Derek Roberts",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bcgov/nr-compliance-enforcement/issues"
  },
  "homepage": "https://github.com/bcgov/nr-compliance-enforcement#readme",
  "dependencies": {
    "@automapper/classes": "^8.8.1",
    "@automapper/core": "^8.8.1",
    "@automapper/nestjs": "^8.8.1",
    "@automapper/pojos": "^8.8.1",
    "@automapper/types": "^6.3.1",
    "@golevelup/ts-jest": "^0.4.0",
    "@nestjs/axios": "^3.0.3",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/testing": "^10.4.4",
    "@nestjs/typeorm": "^10.0.2",
    "automapper": "^1.0.0",
    "axios": "^1.7.4",
    "cache-manager": "^5.7.6",
    "cron": "^3.1.7",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "dotenv": "^16.0.1",
    "escape-html": "^1.0.3",
    "eslint-plugin-n": "^15.7.0",
    "fix-esm": "^1.0.1",
    "form-data": "^4.0.0",
    "geojson": "^0.5.0",
    "https-proxy-agent": "^7.0.6",
    "jest-mock": "^29.6.1",
    "jwks-rsa": "^3.0.1",
    "linq-to-typescript": "^11.0.0",
    "nats": "^2.29.3",
    "nest-winston": "^1.9.2",
    "npm-check-updates": "^17.1.3",
    "nrs-ce-common-types": "^1.0.13",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "path-to-regexp": "^8.0.0",
    "pg": "^8.7.3",
    "react-phone-number-input": "^3.4.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.0.0",
    "rxjs": "^7.8.0",
    "supercluster": "^7.1.5",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.27",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/geojson": "^7946.0.10",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.11.18",
    "@types/passport-jwt": "^3.0.8",
    "@types/supercluster": "^7.1.3",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "faker": "^5.5.3",
    "istanbul-badges-readme": "^1.8.4",
    "jest": "^29.7.0",
    "jest-badges": "^0.1.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^13.1.0",
    "pg-promise": "^11.10.2",
    "prettier": "^2.8.3",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "jest": {
    "automock": false,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      ".module.ts",
      ".entity.ts",
      ".dto.ts"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "cobertura",
      "jest-badges"
    ],
    "testEnvironment": "node",
    "testResultsProcessor": "jest-sonar-reporter",
    "setupFiles": [
      "../test/setupJest.ts"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "./node_modules/.bin/eslint --cache --fix"
  },
  "overrides": {
    "minimist@<1.2.6": "1.2.6"
  }
}
